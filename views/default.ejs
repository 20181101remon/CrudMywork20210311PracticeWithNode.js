<body>
    <h1>Work - List</h1>

    <input type="submit" value="add" onclick="javascript:location.href='add'">

    <table class="table table-hover">
        <tr>
            <th>編號</th>
            <th>程式碼</th>
            <th>日期</th>
            <th>同協人員</th>
        </tr>
        <% for ( var i = 0 ; i < data.length ; i++){ %>
            <tr>
                <td>
                    <%= data[i].id  %>
                </td>
                <td>
                    <%= data[i].mywork  %>
                </td>
                <td>
                    <%= data[i].data  %>
                </td>
                <td>
                    <%= data[i].teammember  %>
                </td>
            </tr>
            <% } %>
    </table>


</body>
</html>